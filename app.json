{
  "name": "amarok-md",
  "description": "amarok md powered by cybxkid",
  "logo": "https://i.hizliresim.com/6Bbj7i.jpg",
  "keywords": ["whatsapp"],
  "repository": "https://github.com/Diegoson/AMAROK-MD",
  "stack": "container",
  "env": {
    "ANTI_LINK": {
      "description": "set true or false",
      "required": false,
      "value": ""
    },
    "ANTILINK_ACTION": {
      "description": "kicked when you send link",
      "required": false,
      "value": ""
    },
    "RMBG_KEY": {
      "description": "Remove bg api key ",
      "required": false,
      "value": ""
    },
    "HANDLERS": {
      "description": "prefix for commands, enter false for no prefix",
      "required": false,
      "value": ""
    },
    "PACKNAME": {
      "description": "Sticker Packname required Example amarok",
      "required": false
    },
    "AUTHOR": {
      "description": "Sticker Pack AUTHOR needed Example Diegoson",
      "required": false
    },
    "SUDO": {
      "description": "sudo numbers with country code (Example 27686882509)",
      "required": false
    },
    "HEROKU_APP_NAME": {
      "description": "Heroku app name, same as above entered",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "Heroku account api key, https://dashboard.heroku.com/account",
      "required": true
    }
  }
}
