{
  "name": "amarok-md",
  "description": "amarok md powered by cybxkid",
  "logo": "https://i.imgur.com/vwNY4lg.jpeg",
  "keywords": ["whatsapp"],
  "repository": "https://github.com/Diegoson/AMAROK-MD",
  "stack": "container",
  "env": {
    "ANTI_LINK": {
      "description": "set true or false",
      "required": false,
      "value": ""
     },
     "SESSION_ID": {
       "description": "session id required add your session id)",
       "required": true,
       "value": ""
    },
    "ANTILINK_ACTION": {
      "description": "kicked when you send link",
      "required": false,
      "value": ""
    },
    "ALIVE": {
      "description": "Alive message {image/link}, {sender} can also be used",
      "required": false,
      "value": "https://i.imgur.com/w5wr6c1.jpeg",
    },
    "RMBG_KEY": {
      "description": "Remove bg api key needed get it here (https://www.remove.bg/api)",
      "required": false,
      "value": ""
    },
    "HANDLERS": {
      "description": "prefix for commands, enter false for no prefix",
      "required": false,
      "value": ""
    },
    "PACKNAME": {
      "description": "Sticker Packname required Example amarok",
      "required": false
    },
    "AUTHOR": {
      "description": "Sticker Pack AUTHOR needed Example Diegoson",
      "required": false
    },
    "SUDO": {
      "description": "sudo numbers with country code (Example 27686882509)",
      "required": false
    },
    "MONGODB_URI": {
      "description": "A secret String for Mongodb Connection.(Required)",
    },
    "HEROKU_APP_NAME": {
      "description": "Heroku app name, same as above entered",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "Heroku account api key, https://dashboard.heroku.com/account",
      "required": true
    }
  }
}
